# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
tab_width = 2

[*.{js,jsx,ts,tsx,json,css,scss,md}]
indent_style = tab
tab_width = 2

[*.{html,xml}]
indent_style = tab
tab_width = 2